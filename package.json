  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@prisma/generator-helper": "^3.14.0",
    "@prisma/sdk": "^3.15.2",
    "@trpc/client": "^9.25.3",
    "@trpc/react": "^9.25.3",
    "@trpc/server": "^9.25.3",
    "prettier": "^2.6.2",
    "tslib": "^2.4.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/prettier": "^2.6.1",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.14",
    "vitest": "^0.18.0"
  },
  "bugs": {
    "url": "https://github.com/omar-dulaimi/prisma-zod-generator/issues"
